# OpenAI API Key - Required for embeddings and chat completions
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Model Settings (optional)
# OPENAI_EMBED_MODEL=text-embedding-3-large
# OPENAI_EMBED_DIM=3072
# OPENAI_CHAT_MODEL=gpt-4o-mini

# Qdrant Settings (optional)
# QDRANT_URL=http://localhost:6333
# QDRANT_COLLECTION=docs
# QDRANT_TIMEOUT=30

# Chunking Settings (optional)
# CHUNK_SIZE=1000
# CHUNK_OVERLAP=200

# Application Settings (optional)
# APP_NAME=RAG Production App
# APP_ENV=development
# LOG_LEVEL=INFO

# Inngest Settings (optional)
# INNGEST_APP_ID=rag_app
# INNGEST_API_BASE=http://127.0.0.1:8288/v1
